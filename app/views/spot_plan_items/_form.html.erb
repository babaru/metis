
<%= simple_form_for(@spot_plan_item , :html => {:class => 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
            
    <%= f.input :master_plan_item_id %>
          
    <%= f.input :placed_at %>
          
    <%= f.input :count %>
          
    <%= f.input :is_enabled %>
          
    <%= f.input :created_by_id %>
                    </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'button button-primary' %>
  </div>
<% end %>
