
<%= simple_form_for(@master_plan_item , :html => {:class => 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
            
    <%= f.input :spot_id %>
          
    <%= f.input :master_plan_id %>
          
    <%= f.input :count %>
                    </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'button button-primary' %>
  </div>
<% end %>
