<div class="narrow-center-page">
<%= render_cell(
  :page_title,
  :show,
  data:
  {
    name: "#{@collection.name}",
    toolbar: {
      left:
      [
        render_cell(
          :button,
          :show,
          data:
          {
            name: fa_icon('level-up rotate-270'),
            url: collections_path,
            options:
            {
              class: 'btn',
              rel: 'tooltip',
              title: "#{t('labels.back_to')}#{t('model.list', :model => Collection.model_name.human)}",
            }
          }
        )
      ]
    }
  }
)
%>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>
        属性
      </th>
      <th>
        值
      </th>
    </tr>
  </thead>
  <tbody>
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.client_id') %>
      </td>
      <td>
        <%= collection.client_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.project_id') %>
      </td>
      <td>
        <%= collection.project_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.amount') %>
      </td>
      <td>
        <%= collection.amount %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.unit') %>
      </td>
      <td>
        <%= collection.unit %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.agency_id') %>
      </td>
      <td>
        <%= collection.agency_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.made_at') %>
      </td>
      <td>
        <%= collection.made_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.credit_on') %>
      </td>
      <td>
        <%= collection.credit_on %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.collection.space_id') %>
      </td>
      <td>
        <%= collection.space_id %>
      </td>
    </tr>
    
        
        
    
  </tbody>
  <tfoot>

  </tfoot>
</table>
</div>
